{
    "layer": "top",
    "position": "top",
    "reload_style_on_change": true,
    "height": 33,
    "width": 1900,
    "margin": "3, 0, 2, 0",
    // Choose the order of the modules
    "modules-left": ["custom/scratchpad-indicator","sway/workspaces","sway/mode","custom/separator"],
    "modules-center": ["mpris"],
    "modules-right": [
        "idle_inhibitor",
        "custom/separator",
        "cpu",
        "custom/separator",
        "memory",
        "custom/separator",
        "pulseaudio",
        "custom/separator",
        "tray",
        "custom/separator",
        "clock"],

    // Modules configuration
    "sway/workspaces": {
        "disable-scroll": false,
        "numeric-first": true,
        "all-outputs": false,
        "format": "{name}",
        "persistent-workspaces": {
          "1": ["DVI-I-1"],
          "2": ["DVI-I-1"],
          "3": ["DVI-I-1"],
          "4": ["DVI-I-1"],
          "5": ["DVI-I-1"],
          "6": ["HDMI-A-1"],
          "7": ["HDMI-A-1"],
          "8": ["HDMI-A-1"],
          "9": ["HDMI-A-1"],
        }
    },
    "custom/scratchpad-indicator": {
        "interval": 3,
        "return-type": "json",
        "exec": "swaymsg -t get_tree | jq --unbuffered --compact-output '(recurse(.nodes[]) | select(.name == \"__i3_scratch\") | .focus) as $scratch_ids | [..  | (.nodes? + .floating_nodes?) // empty | .[] | select(.id |IN($scratch_ids[]))] as $scratch_nodes | if ($scratch_nodes|length) > 0 then { text: \"\\($scratch_nodes | length)\", tooltip: $scratch_nodes | map(\"\\(.app_id // .window_properties.class) (\\(.id)): \\(.name)\") | join(\"\\n\") } else empty end'",
        "format": "{} üóó",
        "on-click": "exec swaymsg 'scratchpad show'",
        "on-click-right": "exec swaymsg 'move scratchpad'"
    },
    "custom/separator": {
        "format": "|",
        "interval": "once",
        "tooltip": false
    },
    "sway/mode": {
        "format": "<span style=\"italic\">{}</span>"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        }
    },
    "tray": {
        "icon-size": 20,
        "spacing": 8
    },
    "clock": {
        "interval": 1,
        "locale": "C", 
        "format": "ÔÄó {:%I:%M %p}",
        "format-alt": "ÔÅ≥ {:%a,%b %d}" // Icon: calendar-alt
    },
    "cpu": {
        "format": "Ôãõ&#8239;{usage}%",
        "tooltip": false,
        "on-click": "kitty -e 'htop'"
    },
    "memory": {
        "interval": 30,
        "format": "ÔÉâ {used:0.2f}GB",

        "max-length": 10,
        "tooltip": false,
        "warning": 70,
        "critical": 90
    },
    "pulseaudio": {
      // "scroll-step": 1, // %, can be a float
      "format": "{icon} {volume}% {format_source}",
      "format-bluetooth": "{icon}Ôäî {volume}% {format_source}",
      "format-bluetooth-muted": "Ôë¶ {format_source}",
      "format-muted": "Ôë¶  {format_source}",
      "format-source": "ÔÑ∞ {volume}%",
      "format-source-muted": "ÔÑ±",
      "ignored-sinkS": ["Easy Effects Sink"],
      "format-icons": {
        "headphone": "ÔÄ•",
        "hands-free": "Ôñê",
        "headset": "üéß",
        "phone": "ÔÇï",
        "portable": "ÔÇï",
        "car": "ÔÜπ",
        "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
      },
      "on-click": "pwvucontrol"
    },
    "mpris": {
      "format": "{player_icon} {dynamic}",
      "format-paused": "{status_icon} <i>{dynamic}</i>",
     	"player-icons": {
    		"default": "‚ñ∂",
    		"mpv": "üéµ"
    	},
    	"status-icons": {
    		"paused": "‚è∏"
    	},
      "interval": 1,
     "ignored-players": ["firefox"]     
    }
}
